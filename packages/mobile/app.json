{
  "expo": {
    "name": "Fighting Tomatoes",
    "slug": "fighting-tomatoes",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fightingtomatoes.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.fightingtomatoes.mobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ],
    "scheme": "fighting-tomatoes",
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
