{
  "expo": {
    "name": "FightCrewApp",
    "slug": "fightcrewapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "splash": {
      "image": "./assets/app-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#181818"
    },
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fightcrewapp.mobile",
      "backgroundColor": "#181818",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.fightcrewapp.mobile",
      "backgroundColor": "#181818",
      "statusBar": {
        "translucent": true,
        "backgroundColor": "#00000000"
      },
      "runtimeVersion": "1.0.0",
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "fightcrewapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#F5C518",
          "sounds": []
        }
      ]
    ],
    "scheme": "fightcrewapp",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4a64b9f8-325e-4869-ab78-9e0674d18b32"
      }
    },
    "owner": "mikeprimak",
    "updates": {
      "url": "https://u.expo.dev/4a64b9f8-325e-4869-ab78-9e0674d18b32"
    }
  }
}
