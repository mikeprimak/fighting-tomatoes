{
  "expo": {
    "name": "FightCrewApp",
    "slug": "fightcrewapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Fight-Crew-Logo.png",
    "splash": {
      "image": "./assets/Fight-Crew-Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#181818"
    },
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fightcrewapp.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Fight-Crew-Logo.png",
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.fightcrewapp.mobile",
      "statusBar": {
        "translucent": true,
        "backgroundColor": "#00000000"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ],
    "scheme": "fightcrewapp",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4a64b9f8-325e-4869-ab78-9e0674d18b32"
      }
    },
    "owner": "mikeprimak"
  }
}
