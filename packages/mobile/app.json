{
  "expo": {
    "name": "Good Fights",
    "slug": "fightcrewapp",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/homescreen-icon.png",
    "splash": {
      "image": "./assets/splash-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#181818"
    },
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.fightcrewapp.mobile",
      "buildNumber": "15",
      "backgroundColor": "#181818",
      "googleServicesFile": "./GoogleService-Info.plist",
      "runtimeVersion": {
        "policy": "appVersion"
      },
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "goodfights",
              "fightcrewapp",
              "com.googleusercontent.apps.499367908516-j03poule51s7sfvpvdufna0upqa3oseg"
            ]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "UIStatusBarStyle": "UIStatusBarStyleLightContent",
        "UIViewControllerBasedStatusBarAppearance": false
      },
      "associatedDomains": [
        "applinks:goodfights.app"
      ],
      "usesAppleSignIn": true
    },
    "android": {
      "splash": {
        "image": "./assets/splash-logo.png",
        "resizeMode": "native",
        "backgroundColor": "#181818"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-foreground-new.png",
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.fightcrewapp.mobile",
      "versionCode": 31,
      "backgroundColor": "#181818",
      "statusBar": {
        "translucent": true,
        "backgroundColor": "#00000000"
      },
      "runtimeVersion": "1.0.0",
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "goodfights"
            },
            {
              "scheme": "fightcrewapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "goodfights.app",
              "pathPrefix": "/verify-email"
            },
            {
              "scheme": "https",
              "host": "goodfights.app",
              "pathPrefix": "/reset-password"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-logo.png",
          "imageWidth": 200,
          "backgroundColor": "#181818"
        }
      ],
      "expo-router",
      "expo-font",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#F5C518",
          "sounds": []
        }
      ],
      "expo-web-browser",
      "@react-native-google-signin/google-signin",
      "expo-apple-authentication"
    ],
    "scheme": "goodfights",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4a64b9f8-325e-4869-ab78-9e0674d18b32"
      }
    },
    "owner": "mikeprimak",
    "updates": {
      "url": "https://u.expo.dev/4a64b9f8-325e-4869-ab78-9e0674d18b32"
    }
  }
}
